        printMatrix(arr);*/

        int[][] array =  new int[4][3];

        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 3; j++) {
                if(j % 2 == 0) {
                    array[i][j] = (int) (Math.random() * 100);
                }
                else {
                    array[i][j] = (int) (Math.random() * 10);
                }
            }
        }

        printMatrix(array);
    }

    public static void printMatrix(int[][] arr) {
        for (int i = 0; i < arr.length; i++) {
            System.out.println(arr[i][0] + " " + arr[i][1] + " " + arr[i][2]);
        }
    }
}
