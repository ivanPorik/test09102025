import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        /*int[][] arr = new int[3][3];

        for (int i = 0; i < arr.length; i++) {
            arr[0][i] = sc.nextInt();
        }
        arr[1][0] =  sc.nextInt();
        arr[2][0] = sc.nextInt();

        for (int i = 1; i < arr.length; i++) {
            arr[i][1] = arr[0][1] + arr[i][0];
            arr[i][2] = arr[0][2] + arr[i][0];
